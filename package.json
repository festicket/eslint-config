{
  "name": "@festicket/eslint-config",
  "version": "4.1.0",
  "description": "Festicket's ESLint config, extending AirBnB",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/festicket/festicket-eslint"
  },
  "scripts": {
    "lint": "eslint index.js .eslintrc.js --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "homepage": "https://github.com/festicket/festicket-eslint",
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.2.0"
  },
  "peerDependencies": {
    "eslint": "^6.3.0",
    "prettier": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.2",
    "prettier": "^2.3.0"
  }
}
